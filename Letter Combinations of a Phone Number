class Solution:
    def letterCombinations(self, digits: str) -> List[str]:
        dict1 = {
            "2":"abc",
            "3":"def",
            "4":"ghi",
            "5":"jkl",
            "6":"mno",
            "7":"pqrs",
            "8":"tuv",
            "9":"wxyz"
        }

        if not digits:
            return []
        
        letters = [dict1[d] for d in digits if d in dict1]

        combos = [''.join(c) for c in product(*letters)]

        return combos
